	"dependencies": {
		"bcryptjs": "^2.4.3",
		"concurrently": "^3.6.0",
		"config": "^3.0.1",
		"express": "^4.17.3",
		"jsonwebtoken": "^8.5.0",
		"mongoose": "^5.1.6"
	},
	"devDependencies": {
		"nodemon": "^1.19.1"
	},
